[Unit]
Description=Fix sound: https://askubuntu.com/questions/864608/audio-crackle-through-headphones
Requires=sys-devices-pci0000:00-0000:00:1f.3-skl_hda_dsp_generic-sound-card0.device
After=sys-devices-pci0000:00-0000:00:1f.3-skl_hda_dsp_generic-sound-card0.device

[Service]
Type=oneshot
ExecStart=/usr/bin/hda-verb /dev/snd/hwC0D0 0x20 SET_COEF_INDEX 0x67
ExecStart=/usr/bin/hda-verb /dev/snd/hwC0D0 0x20 SET_PROC_COEF 0x3000


[Install]
WantedBy=multi-user.target
